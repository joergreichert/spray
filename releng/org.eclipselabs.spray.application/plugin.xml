<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipselabs.spray.application"
            name="%productName">
         <property
               name="windowImages"
               value="icons/Spray-Logo-Icon-16.png,icons/Spray-Logo-Icon-32.png,icons/Spray-Logo-Icon-48.png,icons/Spray-Logo-Icon-64.png,icons/Spray-Logo-Icon-128.png">
         </property>
         <property
               name="aboutImage"
               value="icons/Spray-Logo-Icon-128.png">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupProgressRect"
               value="5,285,490,10">
         </property>
         <property
               name="startupMessageRect"
               value="5,265,490,15">
         </property>
         <property
               name="appName"
               value="%productName">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="cssTheme"
               value="org.eclipse.e4.ui.css.theme.e4_default">
         </property>
         <property
               name="aboutText"
               value="%aboutText">
         </property>
      </product>
   </extension>

</plugin>
